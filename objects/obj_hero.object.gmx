        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Керування гравцем 

switch(keyboard_key)
    {
    case vk_left: 
        image_speed = 0
        if !kortochki &amp;&amp; obj_note_ico.open=false 
        {
            hspeed = -5;
            sprite_index = spr_hero_l;
            image_speed=0.2
        }
 break; // Якщо він не присів, то швидкість вліво -5
    case vk_right: 
        image_speed = 0
        if !kortochki &amp;&amp; obj_note_ico.open=false 
        {
            hspeed = 5; 
            sprite_index = spr_hero
            image_speed=0.2
        } break; // Якщо він не присів, то швидкість вліво 5
    case vk_down: 
        
        if !kortochki &amp;&amp; obj_note_ico.open=false 
        {
            kortochki=true; 
            hspeed=0; 
            image_speed = 0
            if (sprite_index == spr_stop_hero &amp;&amp; image_index == 1 )
            {
                sprite_index = spr_kortochki_true_l;
               
               
            }
             
            if (sprite_index == spr_stop_hero &amp;&amp; image_index == 0)
            {
                sprite_index = spr_kortochki_true; 

            }
            
        } 
        if (sprite_index == spr_kortochki_true_l &amp;&amp;  image_index == 2) || (sprite_index == spr_kortochki_true &amp;&amp;  image_index == 2)
                {
                    image_speed = 0
                }
                else {image_speed = 0.2}
        
                break; //Він присів і поміняв спрайт
    }
// Віджимання лівої клавіші       
if keyboard_check_released(vk_left) = true 
    {
    hspeed = 0; 
    sprite_index = spr_stop_hero;
    image_index = 1;
    image_speed=0;
    }
// Віджимання правої клавіші
if keyboard_check_released(vk_right) = true
    {
    hspeed = 0;
    sprite_index = spr_stop_hero;
    image_index = 0;
    image_speed=0;
    } 
//Віджимання клавіші вниз
if keyboard_check_released(vk_down) = true
    {
        kortochki = false
        if (sprite_index == spr_kortochki_true_l)
        {  
            
            sprite_index = spr_kortochki_false_l; 
            image_speed = 0.2; 
            
        }
       if (image_index == 2 &amp;&amp; sprite_index = spr_kortochki_false_l)
            {
                image_speed = 0
                sprite_index = spr_stop_hero;
                image_index = 1;
            }
        if (sprite_index = spr_kortochki_true)
        {
            sprite_index=spr_kortochki_false; 
            image_speed = 0.2 
        }
        if (image_index == 2 &amp;&amp; sprite_index == spr_kortochki_false)
            {
                image_speed = 0
                sprite_index = spr_stop_hero;
                image_index = 0;
            }
        
    }

</string>
          </argument>
        </arguments>
      </action>
    </event>
